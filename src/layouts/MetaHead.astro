---
const { title } = Astro.props;
---

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width" />
  <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
  <meta name="generator" content={Astro.generator} />
  <title>{title}</title>
  <link
    rel="preload"
    href="/assets/fonts/league-spartan-v15-latin-700.woff2"
    as="font"
    type="font/woff2"
    crossorigin=""
  />
  <link
    rel="preload"
    href="/assets/fonts/league-spartan-v15-latin-regular.woff2"
    as="font"
    type="font/woff2"
    crossorigin=""
  />
</head>
